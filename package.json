  "scripts": {
    "build:sdk": "cross-env ONE_OFF=true NODE_ENV=development \"./node_modules/.bin/lb-sdk\" server/server client/src/app/shared/sdk -l angular2 -d ng2web -i disabled -w enabled",
    "build:ng": "cd client && \"../node_modules/.bin/ng\" build --aot",
    "heroku-postbuild": "cd client && \"../node_modules/.bin/ng\" build --aot -c=$NODE_ENV",
    "lint": "ng lint",
    "start": "node .",
    "posttest": "npm run lint",
    "ng": "ng",
    "upgrade-db": "node ./database/upgrade-db.js",
    "back-test": "./node_modules/.bin/mocha test/ -name '*.js' --recursive",
    "back-unit-test": "cross-env NODE_ENV=unit_test \"./node_modules/.bin/mocha\" './common/**/*.spec.js'",
    "front-test": "cd client && ng test",
    "e2e": "ng e2e"
  },
